{
    "Routes": [
        {
            "DownstreamPathTemplate": "/api/{everything}?format=json",
            "DownstreamScheme": "https",
            "DownstreamHostAndPorts": [
                {
                    "Host": "swapi.dev",
                    "Port": 443
                }
            ],
            "UpstreamPathTemplate": "/star-wars/{everything}",
            "UpstreamHttpMethod": [
                "Get"
            ]
        },
        {
            "DownstreamPathTemplate": "/api/v1/rest/{everything}",
            "DownstreamScheme": "http",
            "DownstreamHostAndPorts": [
                {
                    "Host": "stapi.co",
                    "Port": 80
                }
            ],
            "UpstreamPathTemplate": "/star-trek/{everything}",
            "UpstreamHttpMethod": [
                "Get"
            ]
        }
    ],
    "GlobalConfiguration": {
        "BaseUrl": "http://localhost:5242"
    }
}